{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "npm run build:client && run-p dev:worker dev:client:watch",
    "dev:worker": "vite",
    "dev:client:watch": "vite build --config vite.client.config.ts --watch --mode development",
    "build": "npm run build:worker && npm run build:client",
    "build:worker": "vite build",
    "build:client": "vite build --config vite.client.config.ts",
    "preview": "wrangler pages dev dist --ip 0.0.0.0 --port 3000",
    "deploy": "npm run build && wrangler pages deploy",
    "clean": "rm -rf dist",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@motionone/dom": "^10.18.0",
    "chart.js": "^4.5.1",
    "hono": "^4.10.7",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
